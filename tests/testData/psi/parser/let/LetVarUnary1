  LetStatement(let statement)
    Keyword(keyword)
      PsiElement(identifier)('let')
    PsiWhiteSpace(' ')
    PsiVimScriptElementImpl(assignment statement)
      Variable(variable)
        PsiElement(identifier)('rev')
      PsiWhiteSpace(' ')
      PsiElement(=)('=')
      PsiWhiteSpace(' ')
