VimScriptFile
  LetStatement(let statement)
    Keyword(keyword)
      PsiElement(identifier)('let')
    PsiWhiteSpace(' ')
    PsiVimScriptElementImpl(assignment statement)
      Variable(variable)
        PsiElement(identifier)('var')
      PsiWhiteSpace(' ')
      PsiElement(=)('=')
      PsiWhiteSpace(' ')
      PsiVimScriptElementImpl(unary expression)
        PsiElement(+)('+')
        Variable(variable)
          PsiElement(identifier)('r')
    PsiWhiteSpace(' ')