  LetStatement(let statement)
    Keyword(keyword)
      PsiElement(identifier)('let')
    PsiWhiteSpace(' ')
    PsiVimScriptElementImpl(assignment statement)
      Variable(variable)
        PsiElement(identifier)('var')
      PsiWhiteSpace(' ')
      PsiElement(=)('=')
      PsiWhiteSpace(' ')
      PsiVimScriptElementImpl(mult-div-mod expression)
        Variable(variable)
          PsiElement(identifier)('dividend')
        PsiWhiteSpace(' ')
        PsiElement(/)('/')
        PsiWhiteSpace(' ')
        Variable(variable)
          PsiElement(identifier)('divisor')
    PsiWhiteSpace(' ')